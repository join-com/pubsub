{
  "name": "@join-com/pubsub",
  "version": "0.0.20",
  "description": "Google Cloud PubSub wrapper",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "github:join-com/pubsub",
  "author": "JOIN AG",
  "license": "UNLICENSED",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "makepretty": "prettier --write '**/*.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lerna run build && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "yarn prettier --write",
      "git add"
    ],
    "src/**/*.ts": [
      "npm run lint --fix",
      "git add"
    ]
  },
  "keywords": [
    "pubsub",
    "gcloud"
  ],
  "devDependencies": {
    "husky": "^3.0.9",
    "lerna": "^3.18.4",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2"
  },
  "dependencies": {}
}
